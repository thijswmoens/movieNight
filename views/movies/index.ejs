<h2>Search Movies</h2>
<form action="/movies" method="GET">
    <label>Title</label>
    <input type="text" name="name" value="<%= searchOptions.title %>">
    <button type="submit">Search</button>
</form>
<br>
<% movies.forEach(movie =>{ %> 
    <div>
        <%= movie.title %> 
    </div>
<% }) %> 